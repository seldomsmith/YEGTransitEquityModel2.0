{
  "name": "Transit Equity Dashboard",
  "image": "mcr.microsoft.com/devcontainers/python:3.11",
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "21",
      "installMaven": "false",
      "installGradle": "false"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python"]
    }
  },
  "postCreateCommand": "sudo apt-get update && sudo apt-get install -y libgdal-dev && pip install -r requirements.txt",
  "remoteUser": "vscode"
}
